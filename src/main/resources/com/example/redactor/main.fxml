<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root dividerPositions="0.5, 0.5" type="SplitPane" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <items>
      <SplitPane>
         <items>
            <VBox prefHeight="355.0" prefWidth="147.0">
               <children>
                  <Button fx:id="floatButton" mnemonicParsing="false" style="-fx-background-color: #FF0000;" text="Float" />
                  <Button fx:id="intButton" mnemonicParsing="false" style="-fx-background-color: #00FF00;" text="Int" />
                  <Button fx:id="stringButton" mnemonicParsing="false" style="-fx-background-color: #FF0000;" text="String" />
                  <Button fx:id="addImageButton" mnemonicParsing="false" style="-fx-background-color: #00FF00;" text="Add image" />
                  <Button fx:id="brightnessButton" mnemonicParsing="false" style="-fx-background-color: #0086FF;" text="Brightness" />
                  <Button fx:id="grayFilterButton" mnemonicParsing="false" style="-fx-background-color: #FF0000;" text="Gray" />
                  <Button fx:id="addTextButton" mnemonicParsing="false" style="-fx-background-color: #00FF00;" text="Add text" />
                  <Button fx:id="transformScaleButton" mnemonicParsing="false" style="-fx-background-color: #0086FF;" text="Transform scale" />
                  <Button fx:id="transformMoveButton" mnemonicParsing="false" style="-fx-background-color: #FF0000;" text="Transform move" />
                  <Button fx:id="transformRotateButton" mnemonicParsing="false" style="-fx-background-color: #0086FF;" text="Transform rotate" />
                  <Button fx:id="blurFilterButton" mnemonicParsing="false" style="-fx-background-color: #FF0000;" text="Blur" />
                  <Button fx:id="invertFilterButton" mnemonicParsing="false" style="-fx-background-color: #00FF00;" text="Invert" />
                  <Button fx:id="sepiaFilterButton" mnemonicParsing="false" style="-fx-background-color: #0086FF;" text="Sepia" />
                  <Button fx:id="saveSchemeButton" mnemonicParsing="false" text="Save script" />
                  <Button fx:id="loadSchemeButton" mnemonicParsing="false" text="Load script" />
                  <Button fx:id="saveImageButton" mnemonicParsing="false" style="-fx-background-color: #00FF00;" text="Save" />
               </children>
            </VBox>
         </items></SplitPane>
      <VBox>
         <children>
            <ScrollPane fitToHeight="true" fitToWidth="true" minViewportHeight="100.0" minViewportWidth="100.0" prefViewportHeight="100.0" prefViewportWidth="100.0" VBox.vgrow="ALWAYS">
               <content>
                  <AnchorPane fx:id="nodeContainer" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="5000.0" prefWidth="5000.0" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
            <ImageView fx:id="globalOutputImageView" fitHeight="50.0" fitWidth="100.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" />
         </content>
      </ScrollPane>
   </items>
</fx:root>
